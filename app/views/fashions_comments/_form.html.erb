<%= form_with model: fashions_comment, local: true do |form| %>
  <div class="form-group">
    <%= form.label :user_id %><br>
    <%= form.text_field :user_id, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :text %><br>
    <%= form.text_field :text, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :created_at %><br>
    <%= form.text_field :created_at, class: "form-control" %>
  <div class="form-group">

    <%= form.label :updated_at %><br>
    <%= form.text_field :updated_at, class: "form-control" %>
  </div>
    <%= form.hidden_field :user_id, value: current_user.id %>
    <%= form.submit class: "btn btn-default" %>
<% end %>
